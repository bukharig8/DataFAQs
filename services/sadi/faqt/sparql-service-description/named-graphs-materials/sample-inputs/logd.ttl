@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sd:   <http://www.w3.org/ns/sparql-service-description#> .

<http://logd.tw.rpi.edu/sparql>
   a sd:Service;
   rdfs:comment "Externally-facing production endpoint";
.

<http://logd.tw.rpi.edu:8890/sparql>
   a sd:Service;
   rdfs:comment "Internal production endpoint";
.

<http://labs.mondeca.com/endpoint/lov>
   a sd:Service;
   rdfs:comment "listed at http://labs.mondeca.com/sparqlEndpointsStatus/";
.

<http://example.org/someendpoint>
   a sd:Service;
   rdfs:comment "Example where sd:url should be used if the subject URI is not an endpoint.";
   sd:url <http://data.oceandrilling.org/sparql>;
.
