@prefix prov:     <http://www.w3.org/ns/prov-o/> .
@prefix datafaqs: <http://purl.org/twc/vocab/datafaqs#> .

<> prov:specializationOf <https://github.com/timrdf/DataFAQs/blob/master/doc/examples/default/faqt-brick/epoch.ttl> .

# # # # # # # # # #
_:select_faqt_services # Every time you load this file, we're talking about a new activity (the one you're about to instantiate).
   a prov:Activity;
   prov:used
      <https://raw.github.com/timrdf/DataFAQs/master/services/sadi/core/select-faqts/via-sparql-query-materials/sample-inputs/from-official-sadi-registry.ttl>;
   prov:wasAttributedTo
      <http://sparql.tw.rpi.edu/services/datafaqs/core/select-faqts/via-sparql-query>;
   prov:hadQualifiedAttribution [
      a prov:Attribution;
      prov:hadQualifiedEntity <http://sparql.tw.rpi.edu/services/datafaqs/core/select-faqts/via-sparql-query>;
      prov:adoptedPlan        <https://github.com/timrdf/DataFAQs/blob/master/services/sadi/core/select-faqts/via-sparql-query.rpy>;
   ];
   #prov:generated the RDF graph that via-sparql-query returns (we don't know the name of it yet until we instantiate the epoch).
.

<http://sparql.tw.rpi.edu/services/datafaqs/core/select-faqts/via-sparql-query>
   a datafaqs:FAqTSelector, datafaqs:CoreService, datafaqs:SADIService, prov:Agent;
.


